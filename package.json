{
  "name": "@dzo/com",
  "version": "2.0.6",
  "scripts": {
    "prepare": "dumi setup",
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm run pub",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "pub": "npm run build && sh publish.sh",
    "install-peers": "npm i install-peers"
  },
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "dist"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@ctrl/tinycolor": "^3.6.0",
    "@dzo/utils": "^1.0.4",
    "antd-style": "^3.6.1",
    "classnames": "^2.2.6",
    "compression-webpack-plugin": "^11.1.0",
    "copy-to-clipboard": "^3.3.1",
    "eslint-plugin-html": "^6.1.1",
    "install-peers": "^1.0.4",
    "lodash": "^4.17.20",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "dumi": "^2.0.2",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "antd": "^5.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
